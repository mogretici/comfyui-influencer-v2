      },
      "steps": {
        "label": "Processing Steps",
        "description": "How many steps to generate the image. More steps = more detail but slower. Fewer is usually enough.",
        "recommended": "25 – 35"
      },
      "cfg": {
        "label": "Prompt Adherence",
        "description": "Controls how closely the AI follows your prompt. For Flux models, low values (1.0) work best.",
        "recommended": "1.0",
        "warning": "Flux models work best around 1.0. 2.0+ is not recommended."
      },
